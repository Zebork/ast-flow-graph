{
    "name": "cfg",
    "version": "1.0.0",
    "description": "Constructs a CFG for JavaScript source code.",
    "main": "index.js",
    "scripts": {
        "lint": "eslint index.js lib/**/*.js src/**/*.js",
        "check": "npm run lint -s && dependency-check package.json --entry index.js",
        "test": "istanbul cover node_modules/mocha/bin/_mocha -- ./test/",
        "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "postcoveralls": "rimraf ./coverage",
        "prepublish": "npm test",
        "patch": "npm version patch && npm publish",
        "minor": "npm version minor && npm publish",
        "major": "npm version major && npm publish",
        "postpublish": "git push origin master --follow-tags",
        "changelog": "npx auto-changelog -p -l 10 && git add CHANGELOG.md",
        "marktoc": "npx replace -s -q '### Changelog' '<!-- START doctoc -->\\\n<!-- END doctoc -->' CHANGELOG.md",
        "toc": "npx doctoc --github --title \"### Changelog\" CHANGELOG.md",
        "version": "run-s -s changelog marktoc toc"
    },
    "repository": {
        "types": "git",
        "url": "git+https://github.com/julianjensen/cfg.git"
    },
    "keywords": [
        "cfg",
        "flowgraph"
    ],
    "author": "Julian Jensen <jjdanois@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/julianjensen/cfg/issues"
    },
    "homepage": "https://github.com/julianjensen/cfg#readme",
    "devDependencies": {
        "@types/esprima": "^4.0.1",
        "@types/estree": "^0.0.38",
        "astring": "^1.2.0",
        "chai": "^4.1.1",
        "chalk": "^2.3.0",
        "cli-color": "^1.2.0",
        "command-line-args": "^4.0.7",
        "command-line-usage": "^4.0.2",
        "coveralls": "^2.13.1",
        "dependency-check": "^2.5.1",
        "eslint": "^4.4.0",
        "execa": "^0.8.0",
        "eyes": "^0.1.8",
        "get-stdin": "^5.0.1",
        "istanbul": "^0.4.5",
        "mocha": "^3.5.0",
        "mocha-lcov-reporter": "^1.3.0",
        "npm-run-all": "^4.1.2",
        "rimraf": "^2.4.2",
        "viz.js": "^1.8.0"
    },
    "dependencies": {
        "cli-table2": "^0.2.0",
        "common-tags": "^1.5.1",
        "dominators": "^1.1.1",
        "escodegen": "^1.9.0",
        "escope": "^3.6.0",
        "espree": "^3.5.2",
        "estraverse": "^4.2.0",
        "rocambole": "^0.7.0",
        "sprintf-js": "^1.1.1",
        "traversals": "^1.0.15",
        "typescript": "^2.6.2",
        "yallist": "^3.0.2"
    }
}
